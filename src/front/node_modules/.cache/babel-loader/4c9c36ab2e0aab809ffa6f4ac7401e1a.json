" \r\n                icon=\"trash-fill\" \r\n                variant=\"dark\"\r\n                @click=\"del(row.item, row.index, $event.target)\"></b-icon>\r\n      </template>      \r\n\r\n    </b-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import InterestStockService from \"@/services/interestStock.service.js\";\r\n\r\n  export default {\r\n    name: \"interestStock-list\",\r\n    components: {\r\n      /**\r\n       * components\r\n       */\r\n    },\r\n    data () {\r\n      return {\r\n        // Note 'isActive' is left out and will not appear in the rendered table\r\n        fields: [\r\n          {\r\n            key: 'businessCycle',\r\n            label: '경기순환/섹터',\r\n            sortable: true\r\n          },\r\n          {\r\n            key: 'ticker',\r\n            label: '티커',\r\n            sortable: true\r\n          },\r\n          {\r\n            key: 'currentPrice',\r\n            label: '현재가',\r\n            sortable: false,\r\n            variant: 'danger'\r\n          },\r\n          {\r\n            key: 'desiredPurchasePrice',\r\n            label: '매수희망가',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'avgPrice',\r\n            label: '평균가(1년)',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'under10Price',\r\n            label: '10% 하락',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'under20Price',\r\n            label: '20% 하락',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'under30Price',\r\n            label: '30% 하락',\r\n            sortable: false\r\n          },         \r\n          {\r\n            key: 'w52',\r\n            label: '최저-52주-최고',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'divYield',\r\n            label: '배당수익률',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'fiveYearAvgDividendYield',\r\n            label: '5년평균배당률',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'payoutRatio',\r\n            label: '배당성향',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'dividendGrowth',\r\n            label: '배당성장년수',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'annualPayout',\r\n            label: '배당금',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'dividendPayMonth',\r\n            label: '배당주기',\r\n            sortable: false\r\n          },\r\n          {\r\n            key: 'actions',\r\n            label: 'Actions',\r\n          },          \r\n        ],\r\n        items: [\r\n          { isActive: true, age: 40, first_name: 'Dickerson', last_name: 'Macdonald' },\r\n          { isActive: false, age: 21, first_name: 'Larsen', last_name: 'Shaw' },\r\n          { isActive: false, age: 89, first_name: 'Geneva', last_name: 'Wilson' },\r\n          { isActive: true, age: 38, first_name: 'Jami', last_name: 'Carney' }\r\n        ],\r\n      }\r\n    },\r\n    created() {\r\n\r\n    },\r\n    mounted() {  \r\n      this.getData()\r\n    },\r\n    methods: {\r\n      refresh() {\r\n        //this.getPortfolioList()\r\n        this.getData()\r\n      },\r\n      getData() {\r\n        let param = { email: this.$store.state.email\r\n                    } \r\n        InterestStockService.getList(param)\r\n        .then(res => {\r\n\r\n          console.log(res);\r\n          if (res.status === 200) {\r\n            //this.items = res.data.list\r\n            console.log('res.data='+res.data)\r\n            let tmp_items = []\r\n            res.data.forEach((element, index) => {\r\n              console.log('index='+index+', element='+element)\r\n              \r\n              var obj = {\r\n                interestStockId: element.interestStockId,\r\n                businessCycle: element.stock.businessCycle + '/' + element.stock.sector,\r\n                ticker: element.stock.ticker,\r\n                currentPrice: Number(element.stock.currentPrice).toFixed(2),\r\n                desiredPurchasePrice: 0,  //매수희망가\r\n                avgPrice: 0,  // 1년 평균가\r\n                under10Price: Number(element.stock.highestPrice * 0.9).toFixed(2), // 최고점 대비 10% 하락\r\n                under20Price: Number(element.stock.highestPrice * 0.8).toFixed(2), // 최고점 대비 20% 하락\r\n                under30Price: Number(element.stock.highestPrice * 0.7).toFixed(2), // 최고점 대비 30% 하락\r\n                w52: 0,\r\n                divYield: element.stock.dividendYield,\r\n                fiveYearAvgDividendYield: element.stock.fiveyearAvgDividendYield,\r\n                payoutRatio: element.stock.payoutRatio,\r\n                dividendGrowth: element.stock.dividendGrowthRate,\r\n                annualPayout: element.stock.annualPayout,\r\n                dividendPayMonth: element.stock.dividendPayMonth,\r\n              }\r\n              console.log('obj='+obj)\r\n              tmp_items.push(obj)\r\n              \r\n            })\r\n            this.items = tmp_items\r\n          } else {\r\n          }\r\n        })    \r\n      },      \r\n      del(item, index, event) {\r\n        \r\n        console.log('del::index='+index+', item='+JSON.stringify(item))\r\n        console.log('>>>>>>>>>>>>>>>>>>')\r\n\r\n        if(confirm(\"삭제하시겠습니까?\")) {\r\n          InterestStockService.remove(item.interestStockId)\r\n          .then(\r\n            res => {\r\n              if (res.status === 200) {\r\n                this.refresh()\r\n                alert(\"삭제되었습니다.\")\r\n              } else {\r\n                alert(\"실행중 실패했습니다.\\n다시 이용해 주세요.\")\r\n              }\r\n            })\r\n        }\r\n\r\n      }      \r\n    }\r\n  }\r\n</script>\r\n<style lang=\"scss\">\r\n\r\n</style>    "],"sourceRoot":"src/pages/interestStock"}]}